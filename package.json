{
  "name": "drug_spider",
  "version": "1.0.0",
  "description": "药店020商圈调研工具",
  "main": "dist/main/main.js",
  "homepage": "/",
  "scripts": {
    "dev": "vite",
    "build:vite": "vite build --outDir dist/renderer",
    "build:electron": "electron-builder",
    "build": "npm run build:vite && npm run build:electron",
    "start": "electron ."
  },
  "build": {
    "appId": "com.360zhishu.drug.spider",
    "productName": "Python工具",
    "directories": {
      "output": "release",
      "app": "."
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "asar": true,
      "icon": "src/renderer/assets/logo_256.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "asarUnpack": [
      "dist/main/**/*",
      "dist/renderer/**/*"
    ]
  },
  "author": "Adolph",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@vitejs/plugin-react": "^4.7.0",
    "electron": "^22.3.27",
    "electron-builder": "^24.6.4",
    "glob": "^7.2.3",
    "react": "^15.7.0",
    "vite": "^4.4.9",
    "vite-plugin-commonjs-externals": "^0.1.4",
    "vite-plugin-electron": "^0.29.0",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "antd": "^3.26.20",
    "anyproxy": "^4.1.3",
    "cross-os-proxy": "^2.3.2",
    "file-saver": "^2.0.5",
    "node-schedule": "^2.1.1",
    "react-dom": "^15.7.0",
    "react-router-dom": "^4.3.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.0.2",
    "win-ca": "^3.5.1",
    "xlsx": "^0.18.5"
  }
}
